{
	"name": "ui",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@emotion/css": "^11.11.2",
		"@emotion/styled": "^11.11.0",
		"@mui/icons-material": "^5.8.4",
		"@mui/material": "^5.9.2",
		"axios": "^0.27.2",
		"i18next": "^23.10.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-hook-form": "^7.51.0",
		"react-i18next": "^14.1.0",
		"react-router-dom": "^6.3.0",
		"react-scripts": "5.0.1",
		"react-toastify": "^10.0.4",
		"swr": "^2.2.5",
		"typescript": "^4.9.5",
		"web-vitals": "^2.1.4",
		"zustand": "^4.5.2"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "jest",
		"test:cov": "jest --collectCoverage --maxWorkers=20%",
		"eject": "react-scripts eject",
		"lint": "npx eslint src/**.*ts?(x)"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		],
		"overrides": [
			{
				"files": [
					"**/*.ts?(x)"
				],
				"rules": {
					"no-unused-vars": "warn",
					"no-unused-expressions": "warn"
				}
			}
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^13.4.0",
		"@testing-library/user-event": "^13.5.0",
		"@types/jest": "^27.5.2",
		"@types/node": "^16.18.87",
		"@types/react": "^18.2.64",
		"@types/react-dom": "^18.2.21",
		"jest": "^27.5.1",
		"ts-jest": "^29.1.2"
	}
}
